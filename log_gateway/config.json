{
  "name": "Log Gateway",
  "version": "0.1.8",
  "slug": "log_gateway",
  "description": "Read-only gateway for Supervisor logs (snapshot, no stream).",
  "arch": ["aarch64", "amd64", "armv7", "armhf", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "hassio_api": true,
  "hassio_role": "manager",
  "map": ["config:ro"],
  "homeassistant_api": false,
  "host_network": false,
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Log gateway HTTP (Bearer token)"
  },
  "options": {
    "api_token": "change-me",
    "z2m_slug": "45df7312_zigbee2mqtt",
    "lines_default": 1000,
    "lines_max": 1000,
    "no_colors": true
  },
  "schema": {
    "api_token": "str",
    "z2m_slug": "str",
    "lines_default": "int",
    "lines_max": "int",
    "no_colors": "bool"
  }
}
